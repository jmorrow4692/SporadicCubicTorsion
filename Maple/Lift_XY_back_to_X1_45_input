for i to 5 do
	x_4[i] := c1[i] * subs({c = r*s-s, b = r^2*s-r*s}, {r = (x^2*y-x*y+y-1)/(x^2*y-x), s = (x*y-y+1)/(x*y)},
		WriteInTermsOf( Diamond(4^i,div[7-1]-div[8-1]), div));
	y_4[i] := c2[i] * subs({c = r*s-s, b = r^2*s-r*s}, {r = (x^2*y-x*y+y-1)/(x^2*y-x), s = (x*y-y+1)/(x*y)},
		WriteInTermsOf( Diamond(4^i,div[8-1]-div[9-1]), div));
