Xv := (y-1)^13*(x-y+1)^2*(x^2*y-x*y^2+y-1)^44*(x^2*y-x*y^2-x*y+y^2-1)*(x^2*y-x*y^2-x^2+x*y-x+y-1)*(x^2*y^2-x*y^3-2*x^2*y+2*x*y^2-y+1)*(x^5*y^2-3*x^4*y^3+3*x^3*y^4-x^2*y^5-x^2*y^4+x*y^5+2*x^3*y^2-x^2*y^3-x*y^4-2*x^3*y+x^2*y^2+2*x*y^3-y^4+x^2*y-2*x*y^2+2*y^3-x*y-y^2+x)*(x^4*y-2*x^3*y^2+x^2*y^3-x^3*y+x^2*y^2+2*x^2*y-2*x*y^2-x^2+x*y+y-1)*(x^5*y^2-3*x^4*y^3+3*x^3*y^4-x^2*y^5-x^4*y^2+3*x^3*y^3-3*x^2*y^4+x*y^5-2*x^2*y^2+3*x*y^3-y^4+3*x^2*y-4*x*y^2+y^3+y-1)*(x^5*y^2-2*x^4*y^3+x^3*y^4-x^5*y+3*x^3*y^3-2*x^2*y^4+2*x^4*y-4*x^3*y^2+x^2*y^3+x*y^4+x^2*y^2-x*y^3+x^3-x^2*y+x*y^2-y^3-x*y+2*y^2-y)^43*(x-1)^2*(x^4*y^2-x^3*y^3-6*x^3*y^2+6*x^2*y^3+6*x^3*y-3*x^2*y^2-3*x*y^3-4*x^2*y+4*x*y^2-y^3+x^2+x*y+3*y^2-2*x-3*y+1)*(x*y-1)^11/((x^4*y-3*x^3*y^2+2*x^2*y^3+x^3*y+x^2*y^2-2*x*y^3-2*x^2*y+2*x*y^2+y^2-2*y+1)*(x^7*y^4-3*x^6*y^5+3*x^5*y^6-x^4*y^7-2*x^7*y^3+4*x^6*y^4-4*x^4*y^6+2*x^3*y^7+x^7*y^2-5*x^5*y^4+5*x^4*y^5-x^2*y^7-3*x^5*y^3+6*x^4*y^4-3*x^3*y^5+3*x^5*y^2-x^4*y^3-6*x^3*y^4+3*x^2*y^5+x*y^6-x^5*y-x^4*y^2+2*x^3*y^3+2*x^2*y^4-2*x*y^5-x^4*y+5*x^3*y^2-x^2*y^3-3*x*y^4-x^3*y-7*x^2*y^2+8*x*y^3+4*x^2*y-4*x*y^2+y^3-3*y^2+3*y-1)^43*(x*y-y+1)^5*x^18):

